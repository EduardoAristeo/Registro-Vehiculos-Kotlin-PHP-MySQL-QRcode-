<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Agregar_Vehiculo" android:background="@drawable/fondo2">

    <TextView
            android:text="Matricula QR"
            android:layout_width="222dp"
            android:layout_height="46dp"
            android:id="@+id/ET_MatriculaQR" android:textColor="#ffffff"
            android:background="@drawable/boton_redondeado" android:textAlignment="center" android:textSize="18sp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.582"
            app:layout_constraintVertical_bias="0.542"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="46dp" app:srcCompat="@drawable/qr"
            android:id="@+id/imageView9" android:background="#004c7c"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.179"
            app:layout_constraintVertical_bias="0.542"/>
    <EditText
            android:layout_width="244dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/ET_Caracteristicas"
            android:background="@drawable/boton_redondeado"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF" android:hint="Caracteristicas" android:fontFamily="sans-serif"
            android:textAlignment="center" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.435"/>
    <EditText
            android:layout_width="222dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/ET_Color"
            android:background="@drawable/boton_redondeado"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF" android:hint="Color" android:fontFamily="sans-serif"
            android:textAlignment="center" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.582"
            app:layout_constraintVertical_bias="0.326"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="46dp" app:srcCompat="@drawable/edit"
            android:id="@+id/imageView8" android:background="#004c7c"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.179"
            app:layout_constraintVertical_bias="0.435"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="46dp" app:srcCompat="@drawable/color"
            android:id="@+id/imageView7" android:background="#004c7c"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.179"
            app:layout_constraintVertical_bias="0.326"/>
    <EditText
            android:layout_width="227dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/ET_Marca"
            android:background="@drawable/boton_redondeado"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF" android:hint="Marca" android:fontFamily="sans-serif"
            android:textAlignment="center" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.602"
            app:layout_constraintVertical_bias="0.211"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="46dp" app:srcCompat="@drawable/scooter"
            android:id="@+id/imageView6" android:background="#004c7c"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.179"
            app:layout_constraintVertical_bias="0.211"/>
    <Button
            android:text="GENERAR MATRICULA QR"
            android:layout_width="199dp"
            android:layout_height="wrap_content"
            android:id="@+id/BTTN_GenerarMatricula"
            android:background="@drawable/boton_redondeado2" android:textColor="#FFFFFF" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintVertical_bias="0.664" android:onClick="generarMatriculaQR"/>
    <Button
            android:text="REGISTRAR"
            android:layout_width="199dp"
            android:layout_height="wrap_content"
            android:id="@+id/BTTN_Registrar"
            android:background="@drawable/boton_redondeado" android:textColor="#FFFFFF" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintVertical_bias="0.783" android:onClick="registrarVehiculo"/>
</android.support.constraint.ConstraintLayout>